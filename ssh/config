Host ssh.dev.azure.com
    HostName ssh.dev.azure.com
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_perficient

Host bitbucket.org
    HostName bitbucket.org
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_perficient

Host github.com
    HostName github.com
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa
	
Host *
	IdentitiesOnly yes