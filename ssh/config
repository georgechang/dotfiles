Host ssh.dev.azure.com
    HostName ssh.dev.azure.com
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_hexagon

Host vs-ssh.visualstudio.com
    HostName vs-ssh.visualstudio.com
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa_hexagon

Host github.com
    HostName github.com
    User git
	AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa
	
Host *
    IdentitiesOnly yes
